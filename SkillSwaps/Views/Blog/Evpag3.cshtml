@{ Layout = null;
    ViewBag.Title = "Index";
}

@using System.Web.Optimization
<!doctype html>
<html class="no-js" lang="zxx">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Courses | Education</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="manifest" href="~/Content/site.webmanifest">
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/assets/img/favicon.ico">
    @Styles.Render("~/Content/assets/scss")
    @Styles.Render("~/Content/assets/css")



</head>
<body>
    <!--? Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <img src="~/Content/assets/img/logo/loder.png" alt="">
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start -->
    @Html.Partial("_Header")
    <main>
        <!--? slider Area Start-->
        <section class="slider-area slider-area2">
            <div class="slider-active">
                <!-- Single Slider -->
                <div class="single-slider slider-height2">
                    <div class="container">
                        <div class="row">
                            <div class="col-xl-8 col-lg-11 col-md-12">
                                <div class="hero__caption hero__caption2">
                                    <h1 data-animation="bounceIn" data-delay="0.2s">Evenimente și Workshopuri</h1>
                                    <!-- breadcrumb Start-->
                                    <nav aria-label="breadcrumb">
                                        <ol class="breadcrumb">
                                            <li class="breadcrumb-item"><a href="index.html">Acasă</a></li>
                                            <li class="breadcrumb-item"><a href="#">Evenimente</a></li>
                                        </ol>
                                    </nav>
                                    <!-- breadcrumb End -->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--? Blog Area Start-->
        <section class="blog_area section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 mb-5 mb-lg-0">
                        <div class="blog_left_sidebar">
                            <article class="blog_item">
                                <div class="blog_item_img">
                                    <img class="card-img rounded-0" src="~/Content/assets/img/blog/Startup-World-Cup-Moldova-.jpg" alt="">
                                    <a href="#" class="blog_item_date">
                                        <h3>7</h3>
                                        <p>August</p>
                                    </a>
                                </div>
                                <div class="blog_details">
                                    <a class="d-inline-block" href="~/Blog/Ev11">
                                        <h2 class="blog-head" style="color: #2d2d2d;">Startup World Cup Moldova</h2>
                                    </a>
                                    <p>
                                        Startup World Cup Moldova este competiția regională care oferă startup-urilor din Moldova oportunitatea de a concura pentru un premiu de 1 milion de dolari și de a interacționa cu investitori globali pentru a-și promova ideile inovatoare.
                                    </p>
                                </div>
                            </article>
                            <article class="blog_item">
                                <div class="blog_item_img">
                                    <img class="card-img rounded-0" src="~/Content/assets/img/blog/Startup-Village.jpg" alt="">
                                    <a href="#" class="blog_item_date">
                                        <h3>11-13</h3>
                                        <p>Septembrie</p>
                                    </a>
                                </div>
                                <div class="blog_details">
                                    <a class="d-inline-block" href="~/Blog/Ev12">
                                        <h2 class="blog-head" style="color: #2d2d2d;">Startup Village</h2>
                                    </a>
                                    <p>
                                        Startup Village este un eveniment unic de trei zile care îmbină networkingul, încheierea de afaceri și experiențele culturale în natură.
                                    </p>
                                </div>
                            </article>
                            <article class="blog_item">
                                <div class="blog_item_img">
                                    <img class="card-img rounded-0" src="~/Content/assets/img/blog/DeepTech-Gigahack.jpg" alt="">
                                    <a href="#" class="blog_item_date">
                                        <h3>27-28</h3>
                                        <p>Septembrie</p>
                                    </a>
                                </div>
                                <div class="blog_details">
                                    <a class="d-inline-block" href="~/Blog/Ev13">
                                        <h2 class="blog-head" style="color: #2d2d2d;">DeepTech Gigahack</h2>
                                    </a>
                                    <p>
                                        DeepTech GigaHack, cel mai mare hackathon din Moldova și un eveniment emblematic al Technovator, a reunit aproape 300 de participanți în 57 de echipe pentru o competiție intensivă de 48 de ore.
                                    </p>
                                </div>
                            </article>
                            <article class="blog_item">
                                <div class="blog_item_img">
                                    <img class="card-img rounded-0" src="~/Content/assets/img/blog/Moldova-DevCon-2025.jpg" alt="">
                                    <a href="#" class="blog_item_date">
                                        <h3>10-11</h3>
                                        <p>Octombrie</p>
                                    </a>
                                </div>
                                <div class="blog_details">
                                    <a class="d-inline-block" href="~/Blog/Ev14">
                                        <h2 class="blog-head" style="color: #2d2d2d;">Moldova DevCon 2025</h2>
                                    </a>
                                    <p>
                                        Moldova DevCon 2024, cel mai mare eveniment regional dedicat dezvoltatorilor și pasionaților de tehnologie organizat de Fusion Works.
                                    </p>
                                </div>
                            </article>
                            <article class="blog_item">
                                <div class="blog_item_img">
                                    <img class="card-img rounded-0" src="~/Content/assets/img/blog/Moldova-Business-Forum-III-2025.jpg" alt="">
                                    <a href="#" class="blog_item_date">
                                        <h3>18</h3>
                                        <p>Octombrie</p>
                                    </a>
                                </div>
                                <div class="blog_details">
                                    <a class="d-inline-block" href="~/Blog/Ev15">
                                        <h2 class="blog-head" style="color: #2d2d2d;">Moldova Business Forum III 2025</h2>
                                    </a>
                                    <p>
                                        Evenimentul îl va avea ca invitat special pe Steve Wozniak, cofondator Apple, care va împărtăși cunoștințele sale inovatoare. Participanții vor avea ocazia să interacționeze cu lideri de afaceri și să învețe din experiențele lor.
                                    </p>
                                </div>
                            </article>
                            <nav class="blog-pagination justify-content-center d-flex">
                                <ul class="pagination">
                                    <li class="page-item">
                                        <a href="~/Blog/Evpag2" class="page-link" aria-label="Previous">
                                            <i class="ti-angle-left"></i>
                                        </a>
                                    </li>
                                    <li class="page-item">
                                        <a href="~/Blog/Evpag2" class="page-link">2</a>
                                    </li>
                                    <li class="page-item active">
                                        <a href="~/Blog/Evpag3" class="page-link">3</a>
                                    </li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <div class="blog_right_sidebar">
                            <aside class="single_sidebar_widget search_widget">
                                <form action="#">
                                    <div class="form-group">
                                        <div class="input-group mb-3">
                                            <input type="text" class="form-control" placeholder='Search Keyword'
                                                   onfocus="this.placeholder = ''"
                                                   onblur="this.placeholder = 'Search Keyword'">
                                            <div class="input-group-append">
                                                <button class="btns" type="button"><i class="ti-search"></i></button>
                                            </div>
                                        </div>
                                    </div>
                                    <button class="button rounded-0 primary-bg text-white w-100 btn_1 boxed-btn"
                                            type="submit">
                                        Search
                                    </button>
                                </form>
                            </aside>
                            <aside class="single_sidebar_widget post_category_widget">
                                <h4 class="widget_title" style="color: #2d2d2d;">Categorii</h4>
                                <ul class="list cat-list">
                                    <li>
                                        <a href="#" class="d-flex">
                                            <p>Domenii tehnice & IT</p>
                                            <p>(37)</p>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#" class="d-flex">
                                            <p>Arte & Design</p>
                                            <p>(10)</p>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#" class="d-flex">
                                            <p>Științe exacte</p>
                                            <p>(03)</p>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#" class="d-flex">
                                            <p>Business & Marketing</p>
